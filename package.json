{
  "name": "happy-dev-server",
  "version": "1.3.0-beta.7",
  "description": "无需配置就能能导入 js、css、image 等文件的静态服务器",
  "main": "dist/happy-dev-server.cjs",
  "module": "dist/es/happy-dev-server.mjs",
  "types": "dist/happy-dev-server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/happy-dev-server.d.ts",
      "require": "./dist/happy-dev-server.cjs",
      "import": "./es/dist/happy-dev-server.mjs"
    },
    "./min": {
      "types": "./dist/happy-dev-server.d.ts",
      "require": "./dist/happy-dev-server.min.cjs",
      "import": "./es/dist/happy-dev-server.min.mjs"
    }
  },
  "type": "module",
  "bin": {
    "happy-dev-server": "dist/bin/happy-dev-server.js"
  },
  "scripts": {
    "postinstall": "patch-package",
    "clean": "del-cli dist && del-cli bin",
    "predev": "npm run clean",
    "dev": "cross-env NODE_ENV=development rollup -c ./scripts/build.js -w",
    "prebuild": "npm run clean && npm run build-dts",
    "build-dts": "rollup -c ./scripts/build.dts.js",
    "build": "cross-env NODE_ENV=production rollup -c ./scripts/build.js"
  },
  "engines": {
    "node": ">=14.18.0",
    "npm": ">=6.14.0"
  },
  "homepage": "https://github.com/18023785187/happy-dev-server",
  "keywords": [
    "happy-dev-server",
    "server"
  ],
  "author": "hym",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-typescript": "^7.22.11",
    "@babel/template": "^7.22.15",
    "@babel/types": "^7.22.17",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@vue/compiler-sfc": "^3.3.4",
    "axios": "^1.5.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http": "0.0.1-security",
    "http-proxy-middleware": "^2.0.6",
    "https": "^1.0.0",
    "less": "^4.2.0",
    "node-sass": "^8.0.0",
    "rollup": "^3.28.1",
    "rollup-plugin-replace": "^2.2.0",
    "sass": "^1.66.1",
    "selfsigned": "^2.1.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/babel__core": "^7.20.1",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/less": "^3.0.4",
    "@types/node-sass": "^4.11.4",
    "@types/ws": "^8.5.5",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "patch-package": "^8.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^5.2.2"
  }
}
